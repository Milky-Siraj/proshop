  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
